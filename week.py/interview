

factorial(int firstInt){
    int result = 1;
        for(int i=0; i < firstInt; i++){
        result = result * (firstInt--);
        }
    return result;
}


factoral2(int firstInt){
    if(firstInt == 0){
        System.out.println("It reached zero ")
    }
    firstInt * factoral2(firstInt - 1)
}


